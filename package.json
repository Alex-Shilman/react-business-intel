{
  "name": "react-best-practices",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git://github.com/SvitlanaShepitsena/react-business-intel.git"
  },
  "main": "index.js",
  "config": {
    "storageDatabaseUri": "mysql://user:password@localhost:3306/dbname",
    "appWebpackBaseUrl": "http://localhost:8080/",
    "appWebpackBuildPath": "public/assets",
    "appServerHost": "127.0.0.1",
    "appServerPort": 80
  },
  "scripts": {
    "b": "babel --stage 1 --plugins ./build/babelRelayPlugin app --out-dir lib && webpack",
    "prestart": "npm run build",
    "update-schema": "babel-node --optional es7.asyncFunctions ./scripts/updateSchema.js",
    "app:webpack:start": "webpack-dev-server --hot --inline --progress --colors --quiet ",
    "app:server:start:dev": "nodemon --exec babel-node --stage 1 -- app/server.js",
    "build": "webpack -p --progress",
    "start": "babel-node scripts/server.js",
    "storage:db:migrate": "sequelize db:migrate --url $npm_package_config_storageDatabaseUri --models-path ./storage/models --migrations-path ./storage/migrations",
    "storage:db:rollback": "sequelize db:migrate:undo --url $npm_package_config_storageDatabaseUri --models-path ./storage/models --migrations-path ./storage/migrations",
    "storage:db:seed": "babel-node ./storage/seed.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "jshint .",
    "validate": "npm ls"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.2.0",
    "classnames": "^2.2.1",
    "ejs": "^2.3.4",
    "express": "^4.13.3",
    "express-graphql": "^0.4.0",
    "graphql": "^0.4.12",
    "graphql-relay": "^0.3.4",
    "history": "^1.13.0",
    "install": "^0.4.0",
    "isomorphic-relay": "^0.3.0",
    "isomorphic-relay-router": "^0.3.1",
    "node-fetch": "^1.3.3",
    "nodemon": "^1.8.1",
    "npm": "^3.5.0",
    "open-browser-webpack-plugin": "0.0.1",
    "radium": "^0.15.3",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-helmet": "^2.2.0",
    "react-hot-loader": "^1.3.0",
    "react-pure-render": "^1.0.2",
    "react-relay": "0.5.0",
    "react-router": "^1.0.0",
    "react-skeleton": "^1.0.2",
    "react-youtube": "^4.1.2"
  },
  "devDependencies": {
    "babel": "^5.8.29",
    "babel-core": "^5.8.33",
    "babel-loader": "^5.3.3",
    "babel-relay-plugin": "^0.4.1",
    "webpack": "^1.12.3"
  },
  "pre-commit": [
    "lint",
    "validate",
    "test"
  ]
}
